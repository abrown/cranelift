test run
set enable_simd
target x86_64 skylake has_avx512dq=true

function %imul_i64x2() -> b1 {
block0:
    [-, %xmm1]    v0 = vconst.i64x2 [1 2]
    [-, %xmm2]    v1 = vconst.i64x2 [2 2]
    [-, %xmm19]   v2 = imul v0, v1

    v3 = vconst.i64x2 [2 4]
    v4 = icmp eq v2, v3
    v5 = vall_true v4
    return v5
}
; run
